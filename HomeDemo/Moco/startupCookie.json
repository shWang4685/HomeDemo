[
  {
    "description":"这是一个会返回cookie的请求",
    "request":{
      "uri":"/getCookies",
      "method":"get"
    },
  "response":{
    "cookies":{
      "login":"true"
    },
    "text":"获得登录cookies信息",
    "headers":{
      "Content-Type":"text/html;charset=gbk"
    }
  }
},
  {
    "description":"这是一个带cookie的get请求",
    "request":{
      "uri":"/get/with/cookies",
      "method":"get",
      "cookies":{
        "login":"true"
      }
    },
    "response":{
      "text":"这是一个带cookie的请求响应",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  },
  {
    "description":"这是一个带cookie的post请求",
    "request":{
      "uri":"/post/with/cookies",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "json":{
        "name":"wangshiheng",
        "age":"18"
      }
    },
    "response":{
        "status":"200",
        "json":{
          "wangshiheng":"success",
          "status":"1"
        }
    }
  },
  {
    "description":"这是一个带请求头的请求",
    "request":{
      "uri":"/post/with/header",
      "method":"post",
      "headers":{
        "content-type":"application-json"
      },
      "json":{
        "name":"wangshiheng",
        "age":"18"
      }

    },
    "response":{
      "json":{
        "wangshiheng":"sucess",
        "status":"1"
      }
    }
  }
]